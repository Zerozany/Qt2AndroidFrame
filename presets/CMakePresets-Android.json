{
    "version": 4,
    "configurePresets": [
        {
            "name": "BaseAndroid",
            "hidden": true,
            "generator": "Ninja",
            "cacheVariables": {
                "ANDROID_OPTION": "ON"
            },
            "environment": {
                "JAVA_HOME": "D:/Android Studio/jbr",
                "GRADLE_USER_HOME": "F:/GradleCache"
            }
        },
        {
            "name": "Debug",
            "inherits": [
                "BaseAndroid"
            ],
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "Release",
            "inherits": [
                "BaseAndroid"
            ],
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "QT_ANDROID_SIGN_APK": "ON"
            },
            "environment": {
                "QT_ANDROID_KEYSTORE_PATH": "${sourceDir}/android/android_release.keystore",
                "QT_ANDROID_KEYSTORE_ALIAS": "SonixBeauty",
                "QT_ANDROID_KEYSTORE_STORE_PASS": "12345678",
                "QT_ANDROID_KEYSTORE_KEY_PASS": "12345678"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "AndroidDebug",
            "configurePreset": "Debug",
            "jobs": 8
        },
        {
            "name": "AndroidRelease",
            "configurePreset": "Release",
            "jobs": 8
        }
    ]
}
